  "mjml-boilerplate": {
    "prefix": "mjml-boilerplate",
    "body": [
      "<mjml>",
      "\t<mj-head>",
      "\t\t<mj-title>$0</mj-title>",
      "\t\t<mj-attributes>",
      "\t\t\t$1",
      "\t\t</mj-attributes>",
      "\t</mj-head>",
      "\t<mj-body>",
      "\t\t<mj-section>",
      "\t\t\t<mj-column>",
      "\t\t\t\t$2",
      "\t\t\t</mj-column>",
      "\t\t</mj-section>",
      "\t</mj-body>",
      "</mjml>"
    ],
    "description": "Basic MJML Template",
    "scope": "source.mjml"
  },
  "mjml-render": {
    "prefix": "mjml-render",
    "body": "<!-- mjml-render -->",
    "description": "Magic comment to enable component previewing",
    "scope": "source.mjml"
  },
  "mj-head-render": {
    "prefix": "mj-head-render",
    "body": ["<!-- mj-head-render", "$1", "-->"],
    "description": "Magic comment to enable head elements when used in conjunction with `<!-- mjml-render -->`",
    "scope": "source.mjml"
  },
  "mj-section-column-text": {
    "prefix": "mj-section-column-text",
    "body": [
      "<mj-section>",
      "\t<mj-column>",
      "\t\t<mj-text>$1</mj-text>",
      "\t</mj-column>",
      "</mj-section>"
    ]
  }
